{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "indent": [
            "error",
            4
        ],
        "no-console": 0
    },
    "rulesDirectory": []
}